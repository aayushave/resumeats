<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login_jsp</servlet-name>
        <servlet-class>login_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logAdmin_jsp</servlet-name>
        <servlet-class>logAdmin_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout_jsp</servlet-name>
        <servlet-class>logout_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signup_jsp</servlet-name>
        <servlet-class>signup_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reset_jsp</servlet-name>
        <servlet-class>reset_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reset_admin_jsp</servlet-name>
        <servlet-class>reset_admin_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>get_result_jsp</servlet-name>
        <servlet-class>get_result_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>post_resume_jsp</servlet-name>
        <servlet-class>post_resume_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>result_jsp</servlet-name>
        <servlet-class>result_jsp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login_jsp</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logAdmin_jsp</servlet-name>
        <url-pattern>/log-in</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout_jsp</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup_jsp</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reset_jsp</servlet-name>
        <url-pattern>/reset</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reset_admin_jsp</servlet-name>
        <url-pattern>/re-set</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>get_result_jsp</servlet-name>
        <url-pattern>/get_result</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>post_resume_jsp</servlet-name>
        <url-pattern>/post_resume</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>result_jsp</servlet-name>
        <url-pattern>/result</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
